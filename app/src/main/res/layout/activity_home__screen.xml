<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="cs464.barhop.Home_Screen"
    android:background="@drawable/Home">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/search_button"
        android:layout_marginTop="191dp"
        android:layout_marginBottom="308dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true"
        android:elegantTextHeight="false"
        android:background="@null" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/nearby_button"
        android:elegantTextHeight="false"
        android:background="@null"
        android:layout_marginBottom="126dp"
        android:layout_alignEnd="@+id/search_button"
        android:layout_alignTop="@+id/search_button"
        android:layout_marginTop="155dp"
        android:layout_alignStart="@+id/search_button"
        android:layout_alignBottom="@+id/button2" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button2"
        android:layout_marginBottom="29dp"
        android:elegantTextHeight="false"
        android:background="@null"
        android:layout_alignEnd="@+id/nearby_button"
        android:layout_alignTop="@+id/search_button"
        android:layout_marginTop="303dp"
        android:layout_alignStart="@+id/nearby_button"
        android:layout_alignParentBottom="true" />
</RelativeLayout>
